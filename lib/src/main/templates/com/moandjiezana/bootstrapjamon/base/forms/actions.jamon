<%args>
String save = null;
String cancel = "Cancel";
String cancelHref = "#";
boolean cancelable = true;
</%args>
<div class="form-actions">
    <input type="submit" class="btn btn-primary"<%if save != null %> value="<% save %>"</%if> />
    <%if cancelable %><&|../buttons/a; href=cancelHref &><% cancel %></&></%if>
</div>